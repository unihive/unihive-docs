{
  "name": "unihive.io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel css eleventy browsersync",
    "eleventy": "eleventy --watch",
    "css": "postcss src/static/css/index.css --o site/static/css/index.css --watch",
    "browsersync": "browser-sync start --server 'site' --files 'site' --port 8080 --no-notify --no-open",
    "debug": "set DEBUG=* & eleventy",
    "build": "NODE_ENV=production eleventy && NODE_ENV=production postcss src/static/css/index.css --o site/static/css/index.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unihive/unihive.io.git"
  },
  "keywords": [],
  "author": "Ronen (Ranis) Eizen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/unihive/unihive.io/issues"
  },
  "homepage": "https://github.com/unihive/unihive.io#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "cssnano": "^5.0.10",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "netlify-cms-proxy-server": "^1.3.22",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.0.2"
  },
  "dependencies": {
    "autoprefixer": "^10.4.0",
    "postcss": "^8.3.11"
  }
}
